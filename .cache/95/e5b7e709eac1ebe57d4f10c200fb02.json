   &:hover {\n      background-color: var(--dark-green);\n    }\n  }\n}\n\n.button-group {\n  display: flex;\n  justify-content: center;\n  margin: 1em 0;\n  .button {\n    padding: rem(16) rem(32);\n    border-radius: rem(8);\n    &__clear {\n      margin-right: 1em;\n      color: var(--white);\n      background-color: var(--green);\n      &:hover {\n        background-color: var(--dark-green);\n      }\n    }\n    &__delete:hover {\n      color: var(--red);\n    }\n  }\n}\n","scss/components/_search-bar.scss":"@use \"../util\" as *;\n\n.search-bar {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 0 rem(2);\n  :focus {\n    outline: none;\n  }\n  &__item,\n  &__category {\n    padding: rem(18);\n    border: solid 1px var(--neutral-600) outset;\n  }\n  &__item {\n    border-radius: 100px 0 0 100px;\n  }\n  &__category {\n    border-radius: 0 100px 100px 0;\n  }\n}\n","scss/style.scss":"@use \"globals\";\r\n@use \"components\";\r\n\r\n@use \"../scss/util/\" as *;\r\n\r\n#shopping-list {\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: var(--neutral-200);\r\n  padding: rem(30);\r\n  border-radius: rem(20);\r\n  margin: rem(24) auto;\r\n  :last-child {\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n\r\n.category-group {\r\n  margin-bottom: rem(28);\r\n  .item {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 1em;\r\n    h2 {\r\n      margin: 0 0.5em;\r\n      margin-right: auto;\r\n    }\r\n    .checkmark-group {\r\n      position: relative;\r\n      height: 24px;\r\n      width: 24px;\r\n      //   &:hover {\r\n      //     .checkmark-icon {\r\n      //       display: block;\r\n      //       z-index: 1;\r\n      //     }\r\n      //   }\r\n      .checkmark-icon {\r\n        position: absolute;\r\n        height: 100%;\r\n        width: 100%;\r\n      }\r\n      .circle {\r\n        height: 100%;\r\n        width: 100%;\r\n        border: solid 2px var(--dark-green);\r\n        border-radius: 100px;\r\n        position: absolute;\r\n      }\r\n    }\r\n  }\r\n  .item-active {\r\n    text-decoration: line-through;\r\n    color: var(--neutral-600);\r\n\r\n    .checkmark-icon {\r\n      display: block;\r\n    }\r\n    .circle {\r\n      display: none;\r\n    }\r\n  }\r\n}\r\n\r\n.category-group h2,\r\n.checkmark-group:hover {\r\n  cursor: pointer;\r\n}\r\n\r\n.icon-wrapper {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 3px;\r\n\r\n  .close-icon {\r\n    height: 12px;\r\n    width: 12px;\r\n  }\r\n  &:hover {\r\n    background-color: var(--white);\r\n    border-radius: 0.25em;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n.divider-horizontal {\r\n  background-color: var(--light-green);\r\n  height: 2px;\r\n  margin-bottom: rem(10);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"8f6a42eed96364c4f763f6295acfc297","cacheData":{"env":{}}}